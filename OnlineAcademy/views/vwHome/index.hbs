<section class="news-box top-margin">
    <!-- New Course -->
    <div class="container">
        <h2>
            <span>Hot courses</span>
        </h2>

        <!-- Row -->
        <div class="row">
            {{#each hotCourses}}
            <div class="col-lg-4 col-md-4 col-sm-12">
                <div id="cat-courses" class="newsBox">
                    <div id="course">
                        <div class="thumbnail">
                            <div class="container">
                                <a href="google.com">
                                    <a href="/course/detail/{{this.id}}">
                                        <img class="img-responsive center-block"
                                            src="{{imagePath}}" alt="Course Image">
                                    </a>
                                </a>
                            </div>

                            <div class="caption">
                                <div id="cat-course-caption">
                                    <p>{{this.title}}</p>
                                </div>
                                <div id="cat-course-price">
                                    <p>$ {{this.price}}</p>
                                </div>
                                <div id="cat-course-field">
                                    <p>{{this.fieldName}}</p>
                                </div>
                                <div id="cat-course-lecturer">
                                    <p>{{this.lecturerName}}</p>
                                </div>
                                <div id="cat-course-status">
                                    <p>{{this.status}}</p>
                                </div>
                                <div id="cat-course-review">
                                    <p>
                                        {{this.numOfStudent}} students &nbsp;
                                        <i class="fa fa-user" aria-hidden="true"></i>
                                    </p>
                                    <p>
                                        &nbsp; &nbsp;
                                    </p>
                                    <p>
                                        {{this.likes}} likes &nbsp;
                                        <i class="fa fa-thumbs-up"></i>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            {{/each}}

        </div>
        <!-- /End row -->

    </div>
</section>

